const DuplicateNumbers = [1, 1, 2, 2, 3, 4, 5, 1];

let numMap=DuplicateNumbers.reduce((acc,cur)=>{
    if(!acc.includes(cur)){
       acc.push(cur); 
    } 
   return acc;
    
},[]);
console.log(numMap)




